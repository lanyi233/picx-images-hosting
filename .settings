{
    "imageName": {
        "enableHash": false,
        "addPrefix": {
            "enable": false,
            "prefix": ""
        }
    },
    "compress": {
        "enable": true,
        "encoder": "mozJPEG"
    },
    "imageLinkType": {
        "selected": "GitHub",
        "presetList": {
            "GitHub Pages": {
                "id": "8vmo4hiavg",
                "name": "GitHub Pages",
                "rule": "https://{{owner}}.github.io/{{repo}}/{{path}}"
            },
            "pic.lanyi233.xyz (Cloudflare)": {
                "id": "kxnejxidnc",
                "name": "pic.lanyi233.xyz (Cloudflare)",
                "rule": "https://pic.lanyi233.xyz/{{path}}"
            },
            "GitHub": {
                "id": "2kro6lth1p",
                "name": "GitHub",
                "rule": "https://github.com/{{owner}}/{{repo}}/raw/{{branch}}/{{path}}"
            },
            "jsDelivr": {
                "id": "70a3bv5v9p",
                "name": "jsDelivr",
                "rule": "https://cdn.jsdelivr.net/gh/{{owner}}/{{repo}}@{{branch}}/{{path}}"
            },
            "Statically": {
                "id": "5j3ya41qj1",
                "name": "Statically",
                "rule": "https://cdn.statically.io/gh/{{owner}}/{{repo}}@{{branch}}/{{path}}"
            },
            "ChinaJsDelivr": {
                "id": "4uaoq3e7il",
                "name": "ChinaJsDelivr",
                "rule": "https://jsd.cdn.zzko.cn/gh/{{owner}}/{{repo}}@{{branch}}/{{path}}"
            }
        }
    },
    "imageLinkFormat": {
        "enable": false,
        "selected": "Markdown",
        "presetList": [
            {
                "name": "Markdown",
                "format": "![imageName](imageLink)"
            },
            {
                "name": "HTML",
                "format": "<img src=\"imageLink\" alt=\"imageName\" />"
            },
            {
                "name": "BBCode",
                "format": "[img]imageLink[/img]"
            }
        ]
    },
    "starred": false,
    "watermark": {
        "enable": false,
        "text": "PicX",
        "fontSize": 50,
        "position": "rightBottom",
        "textColor": "#FFFFFF",
        "opacity": 0.5
    },
    "showAnnouncement": false
}